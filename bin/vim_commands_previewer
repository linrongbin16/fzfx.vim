#!/bin/sh

# set -x

fzfx_trim() {
	var="$*"
	# remove leading whitespace characters
	var="${var#"${var%%[![:space:]]*}"}"
	# remove trailing whitespace characters
	var="${var%"${var##*[![:space:]]}"}"
	printf '%s' "$var"
}

read -r -a contents <<< "$1"
if [ $_FZFX_ENABLE_DEBUG -gt 0 ]; then
	echo "DEBUG #:${#contents[@]},\$contents:[$contents]"
fi
